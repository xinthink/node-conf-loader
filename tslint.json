{
  "extends": [
    "tslint:recommended",
    "tslint-config-standard",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "**/*.d.ts"
    ]
  },
  "rules": {
    "member-access": false,
    "interface-name": false,
    "object-literal-sort-keys": false
  }
}
